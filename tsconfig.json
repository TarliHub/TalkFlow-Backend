{
    "compilerOptions": {
        "lib": ["es5", "es6"],
        "target": "es5",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "outDir": "./dist",
        "baseUrl": "./",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "sourceMap": true,
        "paths": {
            "*": ["node_modules/*"]
        }
    }
}
